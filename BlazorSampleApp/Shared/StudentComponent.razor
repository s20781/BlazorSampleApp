<tr>
    <td>
        @Student.FirstName
    </td>
    <td>
        @Student.LastName
    </td>
    <td>
        <button @onclick="@(()=>OnDeleteClicked.InvokeAsync(Student))">Delete</button>
    </td>
</tr>


<h3>StudentComponent</h3>

@code {
    [Parameter]
    public Student Student { get; set; }

    [Parameter]
    public EventCallback<Student> OnDeleteClicked { get; set; }
}
